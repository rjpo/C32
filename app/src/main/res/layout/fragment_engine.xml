<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#000"
    android:orientation="horizontal"
    tools:context="motor.simulador.luisf.c175_2.Engine">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="vertical"
        android:background="#000">

        <TextView
            android:id="@+id/textView10"
            android:layout_width="131dp"
            android:layout_height="29dp"
            android:text="ENGINE TURBO"
            android:textSize="18dp"
            android:textStyle="bold"
            android:gravity="center"
            android:textColor="#ffc400"
            android:layout_marginTop="8dp"
            android:layout_gravity="center"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="20dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_weight="1"
                android:background="#000">

                <TextView
                    android:id="@+id/textView"
                    android:layout_width="105dp"
                    android:layout_height="20dp"
                    android:text="TURBO CHARGER #1"
                    android:textSize="10sp"
                    android:textStyle="bold"
                    android:textColor="#ffc400"
                    android:gravity="center"
                    android:layout_gravity="center"/>

                <com.xw.repo.BubbleSeekBar
                    android:id="@+id/tbc1"
                    android:layout_width="80dp"
                    android:layout_height="20dp"
                    android:background="#212121"
                    app:bsb_max="4096"
                    app:bsb_progress="0"
                    app:bsb_show_thumb_text="true"
                    android:rotation="270"
                    android:layout_gravity="center"
                    android:layout_marginTop="40dp"
                    android:layout_marginBottom="10dp"/>

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="105dp"
                    android:layout_height="24dp"
                    android:text="TURBO CHARGER #3"
                    android:textSize="10sp"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:textColor="#ffc400"
                    android:layout_gravity="center"
                    android:layout_marginTop="30dp"/>

                <com.xw.repo.BubbleSeekBar
                    android:id="@+id/turbo3"
                    android:layout_width="80dp"
                    android:layout_height="20dp"
                    android:background="#212121"
                    app:bsb_max="4096"
                    app:bsb_progress="0"
                    app:bsb_show_thumb_text="true"
                    android:rotation="270"
                    android:layout_gravity="center"
                    android:layout_marginTop="40dp"
                    android:layout_marginBottom="10dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="105dp"
                    android:layout_height="20dp"
                    android:text="TURBO CHARGER #2"
                    android:textSize="10sp"
                    android:textStyle="bold"
                    android:textColor="#ffc400"
                    android:gravity="center"
                    android:layout_gravity="center" />

                <com.xw.repo.BubbleSeekBar
                    android:id="@+id/turbo2"
                    android:layout_width="80dp"
                    android:layout_height="20dp"
                    android:background="#212121"
                    app:bsb_max="4096"
                    app:bsb_progress="0"
                    app:bsb_show_thumb_text="true"
                    android:rotation="270"
                    android:layout_gravity="center"
                    android:layout_marginTop="40dp"
                    android:layout_marginBottom="10dp"/>

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="105dp"
                    android:layout_height="24dp"
                    android:text="TURBO CHARGER #4"
                    android:textSize="10sp"
                    android:textStyle="bold"
                    android:textColor="#ffc400"
                    android:gravity="center"
                    android:layout_gravity="center"
                    android:layout_marginTop="30dp"/>

                <com.xw.repo.BubbleSeekBar
                    android:id="@+id/turbo4"
                    android:layout_width="80dp"
                    android:layout_height="20dp"
                    android:background="#212121"
                    app:bsb_max="4096"
                    app:bsb_progress="0"
                    app:bsb_show_thumb_text="true"
                    android:rotation="270"
                    android:layout_gravity="center"
                    android:layout_marginTop="40dp"
                    android:layout_marginBottom="10dp"/>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView11"
            android:layout_width="208dp"
            android:layout_height="46dp"
            android:layout_gravity="center"
            android:layout_marginTop="8dp"
            android:gravity="center"
            android:text="COMPRESSOR INLET PRESSURES"
            android:textColor="#ffc400"
            android:textSize="18dp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="105dp"
                    android:layout_height="24dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="ATMOSPHERIC PRESSURE"
                    android:textColor="#ffc400"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <com.xw.repo.BubbleSeekBar
                    android:id="@+id/atmos"
                    android:layout_width="80dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="40dp"
                    android:background="#212121"
                    app:bsb_max="4096"
                    app:bsb_progress="0"
                    app:bsb_show_thumb_text="true"
                    android:rotation="270" />

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="105dp"
                    android:layout_height="24dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="30dp"
                    android:gravity="center"
                    android:text="INTAKE MANIFOLD PRESSURE #2"
                    android:textColor="#ffc400"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <com.xw.repo.BubbleSeekBar
                    android:id="@+id/intakeman2"
                    android:layout_width="80dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="40dp"
                    android:background="#212121"
                    app:bsb_max="4096"
                    app:bsb_progress="0"
                    app:bsb_show_thumb_text="true"
                    android:rotation="270"
                    android:textAlignment="center"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="105dp"
                    android:layout_height="24dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="INTAKE MANIFOLD PRESSURE #1"
                    android:textColor="#ffc400"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <com.xw.repo.BubbleSeekBar
                    android:id="@+id/intakeman"
                    android:layout_width="80dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="40dp"
                    android:background="#212121"
                    app:bsb_max="4096"
                    app:bsb_progress="0"
                    app:bsb_show_thumb_text="true"
                    android:rotation="270" />

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>


</LinearLayout>
